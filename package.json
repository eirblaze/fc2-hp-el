{
  "name": "ekgm",
  "version": "1.0.0",
  "description": "node.js系のパッケージ管理やビルダーやテスターやその他いろいろ試す。各ブランチに単体で展開しつつ、組み合わせて実験するときはそれぞれをマージ。",
  "main": "_src/js/index.js",
  "author": "eirblaze <eirblaze@live.jp>",
  "license": "MIT",
  "private": true,
  "config": {
    "scss_dir": "_src/sass",
    "scss_src": "_src/sass/sv4.scss",
    "scss_map": "_src/sass/style.css.map",
    "scss_out": "docs/css/sv4.css",
    "js_dir": "_src/js"
  },
  "scripts": {
    "sass": "yarn build-css+d && yarn postcss+d",
    "sass-r": "yarn build-css+r && yarn postcss+r",
    "sass-w": "yarn build-css+d && yarn build-css+d -w",
    "build-css": "cross-conf-env node-sass --include-path npm_package_config_scss_dir npm_package_config_scss_src npm_package_config_scss_out",
    "build-css+d": "cross-conf-env yarn build-css --output-style expanded --source-map npm_package_config_scss_map --indent-type tab --indent-width 1 --source-comments",
    "build-css+r": "yarn build-css --output-style compressed",
    "postcss-base": "cross-conf-env postcss -o npm_package_config_scss_out npm_package_config_scss_out",
    "postcss+d": "yarn postcss-base --map",
    "postcss+r": "yarn postcss-base --no-map",
    "nodemon-sass": "nodemon -e scss -x \"yarn build-css+d \"",
    "nodemon-postcss": "nodemon -e scss -x \"yarn build-css && yarn autoprefixer \"",
    "js": "webpack --mode=development",
    "js-r": "webpack --mode=production",
    "js-w": "yarn js -- -w",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "start": "webpack-dev-server",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint ./**/*.ts ./**/*.js ./**/*.mjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.19",
    "@types/jquery": "^3.3.31",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "@typescript-eslint/typescript-estree": "^2.1.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.3.1",
    "cross-conf-env": "^1.1.2",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.3.0",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "jquery-color": "^1.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "postcss-normalize-charset": "^4.0.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "vue": "^2.6.6",
    "vue-loader": "^15.6.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "core-js": "3.2.1",
    "postcss-cli": "^6.1.3"
  }
}
